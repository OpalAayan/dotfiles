(
    version: 3,
    windows: (
        
        scale: 8.5,
        items_per_row: 5,

        // 1. Disable the overview (the launcher)
        overview: None,

        // 2. Enable the simple switcher
        switch: (
            // This is the modifier key to hold (e.g., "alt")
            modifier: "alt",
